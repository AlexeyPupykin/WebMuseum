@model IEnumerable<Museum.Models.GetExhibits_Result>

    @foreach (var m in Model)
    {
    <div class="ksponat col-sm-6">
        <div class="ibox">
            <div class="row">
                <div class="col-sm-6">
                    <h4>Название</h4>
                    <h3><strong>@m.Name</strong></h3>
                </div>
                <div class="col-sm-6">
                    <h4>Создатель</h4>
                    <h3>@m.Creator</h3>
                </div>
            </div>
            <hr />
            <div class="row">
                <div class="col-sm-6">
                    <h5>Дата создания</h5>
                    <h4>@m.DateOfCreation</h4>
                </div>
                <div class="col-sm-6">
                    <a data-toggle="collapse" href="#@m.Name" role="button" aria-expanded="false" aria-controls="addExhibition">
                        <strong>Описание</strong>
                    </a>
                    <div class="collapse m-2" id="@m.Name">@m.Description</div>
                </div>
            </div>
            <hr />
            <div class="row">
                <div class="col-sm-6">
                    <h5>Стиль</h5>
                    <h4>@m.Style</h4>
                </div>
                <div class="col-sm-6">
                    <h5>Тип</h5>
                    <h4>@m.Type</h4>
                </div>
            </div>
        </div>
    </div>
    }
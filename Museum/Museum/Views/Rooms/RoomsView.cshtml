@model IEnumerable<Museum.Models.Room>

@{
    ViewBag.Title = "Залы";
}

@foreach (var m in Model)
{
    <div class="room col-sm-4">

        <h2>@m.Name</h2>
        <p>
            @m.Description
        </p>
        <p><a class="btn btn-default" href="@Url.Action("CurrentRoomView", "Rooms", new {Room = m.IDRoom, Name = m.Name})">Что внутри? &raquo;</a></p>
    </div>
}
<div class="row">

</div>